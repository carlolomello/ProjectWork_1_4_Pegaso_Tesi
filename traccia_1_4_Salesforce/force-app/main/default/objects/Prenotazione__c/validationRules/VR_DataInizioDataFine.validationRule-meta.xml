<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR_DataInizioDataFine</fullName>
    <active>true</active>
    <errorConditionFormula>data_fine__c &lt;= data_inizio__c</errorConditionFormula>
    <errorDisplayField>data_fine__c</errorDisplayField>
    <errorMessage>La data fine deve essere maggiore rispetto la data inizio della prenotazione</errorMessage>
</ValidationRule>
